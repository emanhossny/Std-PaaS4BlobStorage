<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Std-PaaS API Web Client for test the cloud storage service</display-name>
  <servlet>
    <servlet-name>StdPersistentStorageClient</servlet-name>
    <servlet-class>fci.cu.std.paas.client.services.persistent.storage.StdPersistentStorageClient</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StdPersistentStorageClient</servlet-name>
    <url-pattern>/StdPersistentStorageClient</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>